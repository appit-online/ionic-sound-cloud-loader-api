{
  "name": "ionic-sound-cloud-loader-api",
  "version": "1.0.0",
  "description": "Search music and download soundcloud music without API in ionic apps",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appit-online/ionic-sound-cloud-loader-api.git"
  },
  "keywords": [
    "soundcloud",
    "scdl",
    "ionic",
    "soundcloud search",
    "soundcloud api",
    "soundcloud-downloader",
    "soundcloud download",
    "soundcloud loader",
    "soundcloud load",
    "ionic soundcloud search",
    "soundcloud music search",
    "soundcloud music urls",
    "angular soundcloud search",
    "soundcloud crawler",
    "soundcloud info",
    "soundcloud information"
  ],
  "funding": {
    "url": "https://paypal.me/dave7117",
    "type": "paypal"
  },
  "author": "AppIT",
  "license": "ISC",
  "files": [
    "lib/**/*"
  ],
  "bugs": {
    "url": "https://github.com/appit-online/ionic-sound-cloud-loader-api/issues"
  },
  "homepage": "https://github.com/appit-online/ionic-sound-cloud-loader-api#readme",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.7",
    "jest": "^30.0.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript":  "~5.5.4"
  },
  "dependencies": {
    "@angular/core": "^18.2.12",
    "@awesome-cordova-plugins/core": "^6.16.0",
    "@awesome-cordova-plugins/http": "^6.16.0",
    "rxjs": "^7.8.1"
  }
}
